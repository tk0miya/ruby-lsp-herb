module RubyLsp
  class Document[T]
    @needs_parsing: bool

    attr_reader source: String

    def initialize: (source: String, version: Integer, uri: URI::Generic, global_state: GlobalState) -> void
  end

  class ERBDocument[T] < Document[T]
  end

  class RubyDocument[T] < Document[T]
  end
end
